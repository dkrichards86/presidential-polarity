html, body, div, span, h1, h2, h3, h4, h5, h6, p, a, img, small, strong, ul, li, article, aside,  header, menu, nav, section
    margin 0
	padding 0
	border 0
	font-size 100%
	font inherit
	vertical-align baseline

article, aside, header, menu, nav, section
    display block

strong
    font-weight 700

p
	margin-top 12px
	margin-bottom 12px

ul
	list-style disc
	padding-left 24px
